{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "env": [
    "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "DATABASE_URL",
    "NEON_DATABASE_URL",
    "EXPO_PUBLIC_ELEVENLABS_API_KEY",
    "EXPO_PUBLIC_REVENUECAT_API_KEY"
  ],
  "functions": {
    "server/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/:path*"
    }
  ]
}
